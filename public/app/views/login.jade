form(name="login" ng-submit="loginUser" ng-controller="LoginCtrl")
	table
		tr
			td
				label Username:
			td
				input(type="text" ng-model="username")
		tr
			td
				label Password:
			td
				input(type="password" ng-model="password")
		tr
			td
				button(ng-click="submit()") Submit
		tr
			td
				button(ng-click="logout()") Log Out
